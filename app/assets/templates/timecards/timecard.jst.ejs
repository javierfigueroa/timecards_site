<div class="timecard_header">
	<h1><%= model.getFullName()  %></h1>
	<label><%= model.getTimespanLabel() %></label>
</div>
<div class="timecard_body row-fluid">
	<div class="span6">
		<div class="row-fluid">
			<label><%= moment(model.get('timestamp_in')) %></label>
		</div>
		<div class="row-fluid">
			<div class="span5">
				<img src="<%= model.get('photo_in_url') %>" style="width:200px;height:200px" />
			</div>
			<div class="span5">
				<div id="map_in" style="width:250px;height:250px"></div>
			</div>
		</div>
	</div>
	<% if (!model.isMissingClockOut()) {  %>
	<div class="span6">
		<div class="row-fluid">
			 <label> <%= moment(model.get('timestamp_out')) %></label>
		</div>
		<div class="row-fluid">
			<div class="span5">
				<img src="<%= model.get('photo_out_url') %>" style="width:200px;height:200px" />
			</div>
			<div class="span5">
				<div id="map_out" style="width:250px;height:250px"></div>
			</div>
		</div>
	</div>
	<% } %>
</div>